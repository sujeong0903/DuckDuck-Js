{
    "manifest_version": 2,
    "name": "어덕편덕",
    "description": "연예인 사진 자동 분류",
    "version": "1.0",

    "sidebar_action": {
      "default_page": "sidebar/sidebar.html",
      "default_icon": "icon/duck-icon.png",

      "default_title": "어덕편덕",
      "use_navigation_bar": true,
      "mobile_user_agent": true
    },
    "background": {
      "scripts": ["script.js", "jquery-3.3.1.min.js"]
    },
    "commands": {
        "toggle": {
            "suggested_key": {
                "default": "Alt+S"
            },
            "description": "서비스 On/Off 단축키"
        },
        "side-page": {
            "suggested_key": {
                "default": "Alt+A"
            },
            "description": "사이드바 On/Off 단축키"
        }
    },
    "icons": {
        "16": "icon/duck-icon.png",
        "48": "icon/duck-icon.png",
        "128": "icon/duck-icon.png"
    },
    "permissions": ["downloads", "storage", "notifications", "<all_urls>" , "contextMenus", "tabs"]
  }
